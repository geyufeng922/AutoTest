[
  {
    "description": "模拟一个没有参数的GET请求",
    "request": {
      "uri": "/getdemo",
      "method": "get"
    },
    "response": {
      "text": "这是一个没有参数的get请求",
      "headers":{
        "Content-Type":"text/html;charset=GBK"
      }
    }
  },
  {
    "description": "这是一个带参数得GET请求",
    "request": {
      "uri": "/getwithparam",
      "method": "get",
      "queries": {
        "name": "胡汉三",
        "age": "18"
      }
    },
    "response": {
      "text": "胡汉三又回来了22211111111",

      "headers":{
        "Content-Type":"text/html;charset=GBK"
      }
    }
  }
]